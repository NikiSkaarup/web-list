{
    "name": "web-list",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "npm run migrate && node build",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "generate": "drizzle-kit generate --config=./drizzle.config.ts",
        "migrate": "node --loader ts-node/esm ./src/lib/server/database/migrate.ts",
        "studio": "drizzle-kit studio --config=./drizzle.config.ts",
        "up": "drizzle-kit up --config=./drizzle.config.ts"
    },
    "devDependencies": {
        "@catppuccin/highlightjs": "^1.0.0",
        "@floating-ui/dom": "^1.6.11",
        "@libsql/client": "^0.14.0",
        "@logtape/logtape": "^0.6.3",
        "@skeletonlabs/skeleton": "^2.10.2",
        "@skeletonlabs/tw-plugin": "^0.4.0",
        "@sveltejs/adapter-auto": "^3.2.5",
        "@sveltejs/kit": "^2.7.2",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@sveltejs/vite-plugin-svelte-inspector": "^3.0.0",
        "@tabler/icons-svelte": "^3.19.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@types/better-sqlite3": "^7.6.11",
        "@types/node": "^22.7.8",
        "@typescript-eslint/eslint-plugin": "^8.11.0",
        "@typescript-eslint/parser": "^8.11.0",
        "autoprefixer": "^10.4.20",
        "dotenv": "^16.4.5",
        "drizzle-kit": "^0.26.2",
        "drizzle-orm": "^0.35.3",
        "drizzle-zod": "^0.5.1",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.46.0",
        "highlight.js": "^11.10.0",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "svelte": "^5.0.5",
        "svelte-check": "^4.0.5",
        "sveltekit-superforms": "^2.20.0",
        "tailwindcss": "^3.4.14",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.0",
        "typescript": "^5.6.3",
        "vite": "^5.4.9",
        "vite-plugin-tailwind-purgecss": "^0.3.3",
        "zod": "^3.23.8"
    }
