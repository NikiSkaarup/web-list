{
    "name": "web-list",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "migrate": "node --loader ts-node/esm ./src/lib/server/db/migrate.ts",
        "start": "npm run migrate && node build",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "generate": "drizzle-kit generate:sqlite --out ./migrations --schema ./src/lib/server/db/schema.ts --breakpoints"
    },
    "devDependencies": {
        "@catppuccin/highlightjs": "^0.1.2",
        "@floating-ui/dom": "^1.2.9",
        "@libsql/client": "^0.1.6",
        "@skeletonlabs/skeleton": "^1.6.2",
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/kit": "^1.20.1",
        "@sveltejs/vite-plugin-svelte": "^2.4.1",
        "@sveltejs/vite-plugin-svelte-inspector": "^1.0.2",
        "@tabler/icons-svelte": "^2.21.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.3",
        "@types/node": "^20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "autoprefixer": "^10.4.14",
        "dotenv": "^16.1.4",
        "drizzle-kit": "^0.18.1",
        "drizzle-orm": "^0.26.5",
        "drizzle-zod": "^0.4.3",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "highlight.js": "^11.8.0",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "svelte": "^3.59.1",
        "svelte-check": "^3.4.3",
        "sveltekit-superforms": "1.0.0-rc.2",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.3",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "winston": "^3.9.0",
        "zod": "^3.21.4"
    },
    "type": "module"
}
