{
  "name": "web-list",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "migrate": "node --loader ts-node/esm ./src/lib/server/database/migrate.ts",
    "start": "npm run migrate && node build",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate": "drizzle-kit generate:sqlite --out ./migrations --schema ./src/lib/server/database/schema.ts --breakpoints"
  },
  "devDependencies": {
    "@catppuccin/highlightjs": "^0.1.2",
    "@floating-ui/dom": "^1.4.2",
    "@libsql/client": "^0.2.1",
    "@skeletonlabs/skeleton": "^1.8.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@sveltejs/vite-plugin-svelte-inspector": "^1.0.2",
    "@tabler/icons-svelte": "^2.22.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/better-sqlite3": "^7.6.4",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "^8.4.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.2",
    "drizzle-orm": "^0.27.0",
    "drizzle-zod": "^0.4.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.31.0",
    "highlight.js": "^11.8.0",
    "nanoid": "^4.0.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^4.0.0-next.2",
    "svelte-check": "^3.4.4",
    "sveltekit-superforms": "^1.1.1",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "type": "module"
}