:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: system-ui;
	--theme-font-family-heading: system-ui;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 2px;
	--theme-rounded-container: 2px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: var(--color-surface-500);
	--on-secondary: var(--color-surface-500);
	--on-tertiary: var(--color-surface-500);
	--on-success: var(--color-surface-500);
	--on-warning: var(--color-surface-500);
	--on-error: var(--color-surface-500);
	--on-surface: var(--color-primary-50);
	/* =~= Theme Colors  =~= */
	/* primary | #b4befe */
	--color-primary-50: 244 245 255; /* ⬅ #f4f5ff */
	--color-primary-100: 240 242 255; /* ⬅ #f0f2ff */
	--color-primary-200: 236 239 255; /* ⬅ #ecefff */
	--color-primary-300: 225 229 255; /* ⬅ #e1e5ff */
	--color-primary-400: 203 210 254; /* ⬅ #cbd2fe */
	--color-primary-500: 180 190 254; /* ⬅ #b4befe */
	--color-primary-600: 162 171 229; /* ⬅ #a2abe5 */
	--color-primary-700: 135 143 191; /* ⬅ #878fbf */
	--color-primary-800: 108 114 152; /* ⬅ #6c7298 */
	--color-primary-900: 88 93 124; /* ⬅ #585d7c */
	/* secondary | #89dceb */
	--color-secondary-50: 237 250 252; /* ⬅ #edfafc */
	--color-secondary-100: 231 248 251; /* ⬅ #e7f8fb */
	--color-secondary-200: 226 246 250; /* ⬅ #e2f6fa */
	--color-secondary-300: 208 241 247; /* ⬅ #d0f1f7 */
	--color-secondary-400: 172 231 241; /* ⬅ #ace7f1 */
	--color-secondary-500: 137 220 235; /* ⬅ #89dceb */
	--color-secondary-600: 123 198 212; /* ⬅ #7bc6d4 */
	--color-secondary-700: 103 165 176; /* ⬅ #67a5b0 */
	--color-secondary-800: 82 132 141; /* ⬅ #52848d */
	--color-secondary-900: 67 108 115; /* ⬅ #436c73 */
	/* tertiary | #9399b2 */
	--color-tertiary-50: 239 240 243; /* ⬅ #eff0f3 */
	--color-tertiary-100: 233 235 240; /* ⬅ #e9ebf0 */
	--color-tertiary-200: 228 230 236; /* ⬅ #e4e6ec */
	--color-tertiary-300: 212 214 224; /* ⬅ #d4d6e0 */
	--color-tertiary-400: 179 184 201; /* ⬅ #b3b8c9 */
	--color-tertiary-500: 147 153 178; /* ⬅ #9399b2 */
	--color-tertiary-600: 132 138 160; /* ⬅ #848aa0 */
	--color-tertiary-700: 110 115 134; /* ⬅ #6e7386 */
	--color-tertiary-800: 88 92 107; /* ⬅ #585c6b */
	--color-tertiary-900: 72 75 87; /* ⬅ #484b57 */
	/* success | #a6e3a1 */
	--color-success-50: 242 251 241; /* ⬅ #f2fbf1 */
	--color-success-100: 237 249 236; /* ⬅ #edf9ec */
	--color-success-200: 233 248 232; /* ⬅ #e9f8e8 */
	--color-success-300: 219 244 217; /* ⬅ #dbf4d9 */
	--color-success-400: 193 235 189; /* ⬅ #c1ebbd */
	--color-success-500: 166 227 161; /* ⬅ #a6e3a1 */
	--color-success-600: 149 204 145; /* ⬅ #95cc91 */
	--color-success-700: 125 170 121; /* ⬅ #7daa79 */
	--color-success-800: 100 136 97; /* ⬅ #648861 */
	--color-success-900: 81 111 79; /* ⬅ #516f4f */
	/* warning | #f9e2af */
